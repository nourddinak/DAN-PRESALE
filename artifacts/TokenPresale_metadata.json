		"devdoc": {
			"details": "A secure and efficient presale smart contract for a token on the Polygon network. Allows users to purchase tokens using either MATIC or USDT. Includes extensive admin controls and security features.",
			"errors": {
				"EnforcedPause()": [
					{
						"details": "The operation failed because the contract is paused."
					}
				],
				"ExpectedPause()": [
					{
						"details": "The operation failed because the contract is not paused."
					}
				],
				"OwnableInvalidOwner(address)": [
					{
						"details": "The owner is not a valid owner account. (eg. `address(0)`)"
					}
				],
				"OwnableUnauthorizedAccount(address)": [
					{
						"details": "The caller account is not authorized to perform an operation."
					}
				],
				"SafeERC20FailedOperation(address)": [
					{
						"details": "An operation with an ERC-20 token failed."
					}
				]
			},
			"events": {
				"FundsWithdrawn(address,uint256,string)": {
					"details": "Emitted when the owner withdraws funds from the contract.",
					"params": {
						"amount": "The amount of funds withdrawn.",
						"currency": "The currency of the funds withdrawn (e.g., \"MATIC\" or \"USDT\").",
						"owner": "The address of the owner."
					}
				},
				"Paused(address)": {
					"details": "Emitted when the pause is triggered by `account`."
				},
				"TokensPurchased(address,uint256,uint256,string)": {
					"details": "Emitted when a user successfully purchases tokens.",
					"params": {
						"amountIn": "The amount of currency (MATIC or USDT) spent.",
						"buyer": "The address of the user who purchased tokens.",
						"currency": "The currency used for the purchase (e.g., \"MATIC\" or \"USDT\").",
						"tokensReceived": "The amount of presale tokens received."
					}
				},
				"Unpaused(address)": {
					"details": "Emitted when the pause is lifted by `account`."
				},
				"UnsoldTokensWithdrawn(address,uint256)": {
					"details": "Emitted when the owner withdraws unsold tokens.",
					"params": {
						"amount": "The amount of unsold tokens withdrawn.",
						"owner": "The address of the owner."
					}
				}
			},
			"kind": "dev",
			"methods": {
				"buyTokensWithMatic(uint256)": {
					"details": "Allows users to purchase tokens using native MATIC.",
					"params": {
						"_tokensToBuy": "The desired amount of presale tokens to purchase."
					}
				},
				"buyTokensWithUsdt(uint256)": {
					"details": "Allows users to purchase tokens using USDT. The user must approve this contract to spend their USDT beforehand.",
					"params": {
						"_tokensToBuy": "The desired amount of presale tokens to purchase."
					}
				},
				"constructor": {
					"details": "The constructor initializes the contract with the presale token address.",
					"params": {
						"_presaleTokenAddress": "The address of the ERC20 token to be sold."
					}
				},
				"emergencyWithdraw()": {
					"details": "Allows the owner to withdraw both MATIC and USDT in a single emergency function call."
				},
				"isPresaleActive()": {
					"details": "Returns the current presale status.",
					"returns": {
						"_0": "A boolean indicating if the presale is active."
					}
				},
				"owner()": {
					"details": "Returns the address of the current owner."
				},
				"pause()": {
					"details": "Allows the owner to pause the contract in case of an emergency. The presale cannot be paused if it has already ended."
				},
				"paused()": {
					"details": "Returns true if the contract is paused, and false otherwise."
				},
				"renounceOwnership()": {
					"details": "Leaves the contract without owner. It will not be possible to call `onlyOwner` functions. Can only be called by the current owner. NOTE: Renouncing ownership will leave the contract without an owner, thereby disabling any functionality that is only available to the owner."
				},
				"setMaticTokenPrice(uint256)": {
					"details": "Sets the price of one presale token in MATIC. The price is denominated in wei.",
					"params": {
						"_newPrice": "The new price of a single token in MATIC."
					}
				},
				"setMinMaxPurchase(uint256,uint256)": {
					"details": "Sets the minimum and maximum token purchase amounts per transaction. The values are in token units, and will be converted to the correct decimals.",
					"params": {
						"_maxAmount": "The maximum token amount to be purchased.",
						"_minAmount": "The minimum token amount to be purchased."
					}
				},
				"setPresaleTimes(uint256,uint256)": {
					"details": "Sets the start and end timestamps for the presale.",
					"params": {
						"_endTime": "The end timestamp in Unix time.",
						"_startTime": "The start timestamp in Unix time."
					}
				},
				"setUsdtTokenPrice(uint256)": {
					"details": "Sets the price of one presale token in USDT. The price is denominated in wei, assuming 6 decimals for USDT.",
					"params": {
						"_newPrice": "The new price of a single token in USDT (1 USDT = 10^6)."
					}
				},
				"transferOwnership(address)": {
					"details": "Transfers ownership of the contract to a new account (`newOwner`). Can only be called by the current owner."
				},
				"unpause()": {
					"details": "Allows the owner to unpause the contract."
				},
				"withdrawMaticFunds()": {
					"details": "Allows the owner to withdraw all collected MATIC."
				},
				"withdrawUnsoldTokens()": {
					"details": "Allows the owner to withdraw any unsold presale tokens from the contract."
				},
				"withdrawUsdtFunds()": {
					"details": "Allows the owner to withdraw all collected USDT."
				}
			},
